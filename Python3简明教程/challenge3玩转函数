import sys


def change(a):

    temp = int(a/60)

    return [temp,a%60]

try:

    number = int(sys.argv[1])

    if number < 0:

        raise ValueError("A value error happened.")

    else:

        list = change(number)

        print("{:2d} H, {:2d} M".format(list[0],list[1]))

except ValueError:

    print("ValueErrorï¼š Input number cannot be negative")

